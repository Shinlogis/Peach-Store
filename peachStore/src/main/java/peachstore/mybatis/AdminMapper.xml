<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Admin">

	<!-- 전체 조회 -->
	<select id="selectAll">
		select *
		from admin
	</select>
		
	<!-- pk로 조회 -->
	<select id="selectById">
		select *
		from admin
		where admin_id = #{adminId}
	</select>
</mapper>


